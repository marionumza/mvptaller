# STATIC CONFIGURATION

log:
  level: INFO

api:
  dashboard: false

entryPoints:
  http:
    address: ":80"
  https:
    address: ":443"

providers:
  docker:
    #endpoint: "unix:///var/run/docker.sock"
    endpoint: "http://dockersocket:2375"
    exposedByDefault: false

certificatesResolvers:
  letsencrypt:
    acme:
      email:
      storage: acme.json
      httpChallenge:
        entryPoint: http
